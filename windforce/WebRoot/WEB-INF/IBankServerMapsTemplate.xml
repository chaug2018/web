		<PARAM NAME="tracepath" VALUE="/home/logs/ibank" DESC="记报文内容日志的目录，默认为C:\TRACE\SERVER" />		
		<!-- 服务 -->
		<PARAM NAME="ibankrevise_service" VALUE="IBANKREVISE" DESC="字段格式整理服务" />
