<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="ManageAction" name="/ManageAction"
		class="com.yzj.pam.action.ManageAction" scope="prototype" >
		<property name="manageServer">
			<ref bean="ManageServer" />
		</property>
	</bean>
	
	<bean id="DateSource" name="/DateSource"
		class="com.yzj.pam.action.DateSourceAction" >
	</bean>
	
</beans>